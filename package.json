{
  "version": "1.0.0",
  "name": "richardojs",
  "title": "richardoJS",
  "description": "Vanilla Js helpers set built in ES6 by Richardo for the www",
  "main": "index.js",
  "repository": "git@github.com:ricardo-valente/richardoJS.git",
  "author": "Richardo Valente",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^5.0.1",
    "express": "^4.16.1",
    "pug": "^2.0.3",
    "supervisor": "^0.12.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-minify": "^0.4.3",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.24.5",
    "concat-cli": "^4.0.0",
    "cp-cli": "^1.1.2",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.3.0",
    "postcss-cli-simple": "^1.0.4",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "setup": "bash ./bash/run setup",
    "build:server": "bash ./bash/run server",
